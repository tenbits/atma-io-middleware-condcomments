import * as Midd from '../index'
import * as io from 'atma-io'


Midd.setOptions({
	defines: {
		TEST: true
	}
});

io.File.registerExtensions({
	"js": [
		[ Midd, 'read']
	], 	
});

UTest({
	'should remove uncommented block and uncomment the commented one' () {
		eq_(io.File.read('/test/fixtures/foo.js').trim(), 'commented');
	}
})
